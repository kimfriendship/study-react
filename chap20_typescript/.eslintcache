ccess: <T, E>(data: T): AsyncState<T, E> => ({\n    data,\n    loading: false,\n    error: null,\n  }),\n  error: <T, E>(error: E): AsyncState<T, E> => ({\n    error,\n    loading: false,\n    data: null,\n  }),\n};\n\ntype AnyAsyncActionCreator = AsyncActionCreatorBuilder<any, any, any>;\n\nexport function transformToArray<AC extends AnyAsyncActionCreator>(\n  asyncActionCreator: AC\n) {\n  const { request, success, failure } = asyncActionCreator;\n  return [request, success, failure];\n}\n\nexport function createAsyncReducer<\n  S,\n  AC extends AnyAsyncActionCreator,\n  K extends keyof S\n>(asyncActionCreator: AC, key: K) {\n  return (state: S, action: ActionType<AC>) => {\n    const [request, success, failure] = transformToArray(\n      asyncActionCreator\n    ).map(getType);\n\n    switch (action.type) {\n      case request:\n        return {\n          ...state,\n          [key]: asyncState.load(),\n        };\n      case success:\n        return {\n          ...state,\n          [key]: asyncState.success(action.payload),\n        };\n      case failure:\n        return {\n          ...state,\n          [key]: asyncState.error(action.payload),\n        };\n      default:\n        return state;\n    }\n  };\n}\n","/Users/friendship/Documents/dev/study-react/chap20_typescript/src/modules/github/sagas.ts",["136","137","138"],"/Users/friendship/Documents/dev/study-react/chap20_typescript/src/lib/createAsyncSaga.ts",[],{"ruleId":"139","severity":1,"message":"140","line":9,"column":8,"nodeType":"141","messageId":"142","endLine":9,"endColumn":13},{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"139","severity":1,"message":"147","line":1,"column":10,"nodeType":"141","messageId":"142","endLine":1,"endColumn":18},{"ruleId":"139","severity":1,"message":"148","line":1,"column":10,"nodeType":"141","messageId":"142","endLine":1,"endColumn":15},{"ruleId":"139","severity":1,"message":"149","line":3,"column":10,"nodeType":"141","messageId":"14